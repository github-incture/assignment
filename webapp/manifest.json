{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.incture.project1",
    "type": "application",
    "dataSources": {
      "xmlDataNorthWind": {
        "uri": "/destinations/Assignment",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    },
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.4",
      "toolsId": "8e6e84a3-31db-450f-8977-7edd5c0b04cc"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.8",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.incture.project1.i18n.i18n",
          "supportedLocales": ["en", "ar", "fr"],
          "fallbackLocale": "en"
        }
      },
      "languageModel":{
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "languageData"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.incture.project1.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": "home",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "RouteSample",
          "pattern": "sample",
          "target": [
            "TargetSample"
          ]
        },
        {
          "name": "RouteSample2",
          "pattern": "sample2",
          "target": [
            "TargetSample2"
          ]
        },
        {
          "name": "RouteSample3",
          "pattern": "sample3",
          "target": [
            "TargetSample3"
          ]
        },
        {
          "name": "RouteForms",
          "pattern": "forms",
          "target": [
            "TargetForms"
          ]
        },
        {
          "name": "RouteLabel",
          "pattern": "label",
          "target": [
            "TargetLabel"
          ]
        },
        {
          "name": "RouteBinding",
          "pattern": "binding",
          "target": [
            "TargetBinding"
          ]
        },
        {
          "name": "RouteXMLBinding",
          "pattern": "xmlbinding",
          "target": [
            "TargetXMLBinding"
          ]
        },
        {
          "name": "RouteObjectList",
          "pattern": "objectlist",
          "target": [
            "TargetObjectList"
          ]
        },
        {
          "name": "RouteSales",
          "pattern": "sales",
          "target": [
            "TargetSales"
          ]
        },
        {
          "name": "RouteShow",
          "pattern": "",
          "target": [
            "TargetShow"
          ]
        },
        {
          "name": "RouteFragView",
          "pattern": "fragview",
          "target": [
            "TargetFragView"
          ]
        },
        {
          "name": "RouteLanguage",
          "pattern": "langauge",
          "target": [
            "TargetLanguage"
          ]
        }
      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home"
        },
        "TargetSample": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Sample",
          "viewName": "Sample"
        },
        "TargetSample2": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Sample2",
          "viewName": "Sample2"
        },
        "TargetSample3": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Sample3",
          "viewName": "Sample3"
        },
        "TargetForms":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Forms",
          "viewName": "Forms"
        },
        "TargetLabel":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Label",
          "viewName": "Label"
        },
        "TargetBinding":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Binding",
          "viewName": "Binding"
        },
        "TargetXMLBinding":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "XMLBinding",
          "viewName": "XMLBinding"
        },
        "TargetObjectList":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ObjectList",
          "viewName": "ObjectList"
        },
        "TargetSales":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Sales",
          "viewName": "Sales"
        },
        "TargetShow":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Show",
          "viewName": "Show"
        },
        "TargetFragView":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "FragmentView",
          "viewName": "FragmentView"
        },
        "TargetLanguage":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Language",
          "viewName": "Language"
        }
      }
    },
    "rootView": {
      "viewName": "com.incture.project1.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
